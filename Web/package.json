{
  "name": "@open-source-education/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "Client/packages/**"
  ],
  "scripts": {
    "start": "vite --host",
    "lint": "eslint . --fix --max-warnings=0",
    "extract": "formatjs extract 'Client/**/*.{ts,tsx,jsx,js}' --ignore='**/*.d.ts' --out-file ./Messages/Extracted/en.json",
    "format": "prettier . --write",
    "plop": "plop",
    "postinstall": "cd .. && husky install Web/.husky",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "precommit": "yarn run lint && yarn run format",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "syncpack:fix-mismatches": "syncpack fix-mismatches",
    "syncpack:lint": "syncpack lint",
    "syncpack:update": "syncpack update"
  },
  "dependencies": {
    "@lexical/react": "^0.29.0",
    "axios": "^1.8.4",
    "i": "^0.3.7",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-intl": "^7.1.10",
    "react-router": "^7.4.1",
    "react-router-dom": "^7.4.1",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@formatjs/cli": "^6.6.3",
    "@formatjs/ts-transformer": "^3.13.34",
    "@playwright/test": "^1.51.1",
    "@storybook/addon-essentials": "^8.6.11",
    "@storybook/addon-interactions": "^8.6.11",
    "@storybook/addon-links": "^8.6.11",
    "@storybook/addon-onboarding": "^8.6.11",
    "@storybook/blocks": "^8.6.11",
    "@storybook/builder-vite": "^8.6.11",
    "@storybook/react": "^8.6.11",
    "@storybook/react-vite": "^8.6.11",
    "@storybook/test": "^8.6.11",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@types/prop-types": "^15.7.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "babel-plugin-formatjs": "^10.5.37",
    "buffer": "^6.0.3",
    "eslint": "^9.23.0",
    "eslint-config-love": "^119.0.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-node-single-context": "^29.4.0",
    "lint-staged": "^15.5.0",
    "pinst": "^3.0.0",
    "plop": "^4.0.1",
    "prettier": "^3.5.3",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "sass": "^1.86.0",
    "storybook": "^8.6.11",
    "syncpack": "^13.0.3",
    "ts-jest": "^29.3.0",
    "vite": "^6.2.3"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "yarn run lint",
    "*.--write": "yarn run format"
  }
}
