{
  "name": "web",
  "packageManager": "yarn@4.3.0",
  "scripts": {
    "start": "vite --host",
    "lint": "eslint . --fix --max-warnings=0",
    "extract": "formatjs extract 'Client/**/*.{ts,tsx,jsx,js}' --ignore='**/*.d.ts' --out-file ./Messages/Extracted/en.json",
    "format": "prettier . --write",
    "postinstall": "cd .. && husky install Web/.husky",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "precommit": "yarn run lint && yarn run format",
    "test": "jest"
  },
  "dependencies": {
    "@lexical/react": "^0.16.0",
    "axios": "^1.7.2",
    "eslint-plugin-jest": "^28.6.0",
    "i": "^0.3.7",
    "lexical": "^0.16.0",
    "npm": "^10.8.1",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primereact": "^10.6.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^6.6.8",
    "react-rnd": "^10.4.11",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "typescript": "^5.4.5",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@formatjs/cli": "^6.2.12",
    "@formatjs/ts-transformer": "^3.13.14",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "babel-plugin-formatjs": "^10.5.16",
    "buffer": "^6.0.3",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.8.1",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-react": "^7.34.2",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-node-single-context": "^29.4.0",
    "lint-staged": ">=10",
    "pinst": "^3.0.0",
    "prettier": "^3.3.1",
    "process": "^0.11.10",
    "sass": "^1.77.4",
    "ts-jest": "^29.1.4",
    "vite": "^5.2.13"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "yarn run lint",
    "*.--write": "yarn run format"
  },
  "resolutions": {
    "svgo": "^3.2.0"
  }
}
