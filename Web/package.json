{
  "name": "web",
  "packageManager": "yarn@4.3.0",
  "type": "module",
  "workspaces": [
    "Client/packages/**/*"
  ],
  "scripts": {
    "start": "vite --host",
    "lint": "eslint . --fix --max-warnings=0",
    "extract": "formatjs extract 'Client/**/*.{ts,tsx,jsx,js}' --ignore='**/*.d.ts' --out-file ./Messages/Extracted/en.json",
    "format": "prettier . --write",
    "plop": "plop",
    "postinstall": "cd .. && husky install Web/.husky",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "precommit": "yarn run lint && yarn run format",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@lexical/react": "^0.16.1",
    "axios": "^1.7.2",
    "eslint-config-love": "^56.0.0",
    "eslint-plugin-jest": "^28.6.0",
    "i": "^0.3.7",
    "npm": "^10.8.2",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primereact": "^10.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^6.6.8",
    "react-router": "^6.25.1",
    "react-router-dom": "^6.25.1",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@chromatic-com/storybook": "^1.6.1",
    "@formatjs/cli": "^6.2.12",
    "@formatjs/ts-transformer": "^3.13.14",
    "@playwright/test": "^1.45.2",
    "@storybook/addon-essentials": "^8.2.6",
    "@storybook/addon-interactions": "^8.2.6",
    "@storybook/addon-links": "^8.2.6",
    "@storybook/addon-onboarding": "^8.2.6",
    "@storybook/blocks": "^8.2.6",
    "@storybook/builder-vite": "^8.2.6",
    "@storybook/react": "^8.2.6",
    "@storybook/react-vite": "^8.2.6",
    "@storybook/test": "^8.2.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "@types/prop-types": "^15.7.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "babel-plugin-formatjs": "^10.5.16",
    "buffer": "^6.0.3",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-promise": "^6.5.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.1",
    "jest": "^29.7.0",
    "jest-environment-node-single-context": "^29.4.0",
    "lint-staged": "^15.2.7",
    "pinst": "^3.0.0",
    "plop": "^4.0.1",
    "prettier": "^3.3.3",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "sass": "^1.77.8",
    "storybook": "^8.2.6",
    "ts-jest": "^29.2.3",
    "vite": "^5.3.4"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "yarn run lint",
    "*.--write": "yarn run format"
  },
  "resolutions": {
    "svgo": "^3.2.0"
  }
}
