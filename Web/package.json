{
	"name": "Web",
	"packageManager": "yarn@4.0.2",
	"scripts": {
		"start": "parcel Client/index.html",
		"lint": "eslint . --fix --max-warnings=0",
		"extract": "formatjs extract 'Client/**/*.{ts,tsx,jsx,js}' --ignore='**/*.d.ts' --out-file ./Messages/Extracted/en.json",
		"format": "prettier . --write",
		"postinstall": "husky install",
		"prepublishOnly": "pinst --disable",
		"postpublish": "pinst --enable",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable",
		"precommit": "yarn run lint && yarn run format"
	},
	"dependencies": {
		"@emotion/react": "^11.11.3",
		"@emotion/styled": "^11.11.0",
		"@fontsource/roboto": "^5.0.8",
		"@mui/icons-material": "^5.15.2",
		"@mui/material": "^5.15.2",
		"axios": "^1.6.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-intl": "^6.5.5",
		"react-router": "^6.21.2",
		"react-router-dom": "^6.21.2",
		"typescript": "^5.3.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.23.4",
		"@babel/core": "^7.23.7",
		"@babel/preset-env": "^7.23.8",
		"@babel/preset-typescript": "^7.23.3",
		"@formatjs/cli": "^6.2.4",
		"@formatjs/ts-transformer": "^3.13.9",
		"@parcel/core": "^2.10.3",
		"@types/react": "^18.2.46",
		"@types/react-dom": "^18.2.18",
		"@typescript-eslint/eslint-plugin": "^6.19.0",
		"@typescript-eslint/parser": "^6.19.0",
		"babel-plugin-formatjs": "^10.5.10",
		"buffer": "^6.0.3",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard-with-typescript": "^43.0.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^16.6.2",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.33.2",
		"husky": "^8.0.0",
		"lint-staged": ">=10",
		"parcel": "^2.10.3",
		"pinst": "^3.0.0",
		"prettier": "^3.2.2",
		"process": "^0.11.10"
	},
	"lint-staged": {
		"*.{js,ts,jsx,tsx}": "yarn run lint",
		"*.--write": "yarn run format"
	}
}
