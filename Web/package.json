{
  "name": "web",
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "start": "vite --host",
    "lint": "eslint . --fix --max-warnings=0",
    "extract": "formatjs extract 'Client/**/*.{ts,tsx,jsx,js}' --ignore='**/*.d.ts' --out-file ./Messages/Extracted/en.json",
    "format": "prettier . --write",
    "postinstall": "cd .. && husky install Web/.husky",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "precommit": "yarn run lint && yarn run format",
    "test": "jest"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@lexical/react": "^0.13.1",
    "@types/react-grid-layout": "^1.3.5",
    "axios": "^1.6.3",
    "eslint-plugin-jest": "^27.6.3",
    "i": "^0.3.7",
    "lexical": "^0.13.1",
    "npm": "^10.4.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primereact": "^10.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.4.4",
    "react-intl": "^6.5.5",
    "react-resizable": "^3.0.5",
    "react-resize-detector": "^10.0.1",
    "react-rnd": "^10.4.11",
    "react-router": "^6.21.2",
    "react-router-dom": "^6.21.2",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@formatjs/cli": "^6.2.4",
    "@formatjs/ts-transformer": "^3.13.9",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "babel-plugin-formatjs": "^10.5.10",
    "buffer": "^6.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-node-single-context": "^29.2.0",
    "lint-staged": ">=10",
    "pinst": "^3.0.0",
    "prettier": "^3.2.2",
    "process": "^0.11.10",
    "sass": "^1.70.0",
    "ts-jest": "^29.1.1",
    "vite": "^5.1.4"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "yarn run lint",
    "*.--write": "yarn run format"
  },
  "resolutions": {
    "svgo": "^3.2.0"
  }
}
